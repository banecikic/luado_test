<p id="notice"><%= notice %></p>

<h1>Project Phases</h1>

<table>
  <thead>
    <tr>
      <th>Job title</th>
      <th>Price</th>
      <th>Timeframe</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_phases.each do |project_phase| %>
      <tr>
        <td><%= project_phase.job_title %></td>
        <td><%= project_phase.price %></td>
        <td><%= project_phase.timeframe %></td>
        <td><%= project_phase.project.project_title %></td>
        <td><%= link_to 'Show', project_project_phases_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project Phase', new_project_project_phase_path %>
